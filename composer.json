{
    "name": "Responsilicious/alexa-app",
    "description": "Set of classes to make creating simple Amazon Echo Alexa Apps easier with Lumen and to a lesser extent Laravel",
    "license": "MIT",
    "keywords": ["echo", "Amazon Echo", "Alexa", "AlexaApp", "Lumen", "Laravel"],
    "authors": [
        {
            "name": "Kevin Mitchell",
            "email": "kevin@develpr.com"
        }
    ],
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/Responsilicious/alexa-app"
        }
    ],
    "require": {
         "php": "~7.0"
     },
    "require-dev": {
        "phpunit/phpunit": "~5.7",
        "mockery/mockery": "^0.9.9"
    },
    "autoload": {
        "psr-4": {
            "Responsilicious\\AlexaApp\\": "src"
        }
    },
    "autoload-dev": {
        "classmap": [
            "tests/"
        ],
        "psr-4": {
            "Responsilicious\\Tests\\": "tests"
        }
    },
    "extra": {
        "branch-alias": {
            "master": "stable"
        }
    }
}
